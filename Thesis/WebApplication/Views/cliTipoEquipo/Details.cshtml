@model WebApplication.Models.cli_tipoequipo

@{
    ViewBag.Title = "Details";
}
@if (ViewBag.Verificar is Int32)
{
<div class="register-box">
    <div class="register-box-body">
        <h3 style="text-align:center">DETALLE TIPO EQUIPO</h3>

        <div>
            <hr />
            <dl class="dl-horizontal">
                <dt>
                    <label for="inputEmail3" class="col-sm-2 control-label">Descripción</label>
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Cli_Descripcion)
                </dd>

            </dl>
        </div>
        <p>
            @Html.ActionLink("Edit", "Edit", new { id = Model.idCli_TipoEquipo }, new { @class = "btn btn-success" })
            @Html.ActionLink("Volver", "Index", null, new { @class = "btn btn-danger" })
        </p>
    </div>
</div>

}