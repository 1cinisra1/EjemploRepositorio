@model WebApplication.Models.comp_ruta

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div class="register-box">
    <div class="register-box-body">
        <p class="login-box-msg">Detalle</p>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                <label for="inputEmail3" class="col-sm-2 control-label">Fecha</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Comp_Fecha)
            </dd>

            <dt>
                <label for="inputEmail3" class="col-sm-2 control-label">Visita No.</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Comp_NumeroVisitaMes)
            </dd>

            <dt>
                <label for="inputEmail3" class="col-sm-2 control-label">Tiempo Estimado</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Comp_TiempoDur)
            </dd>

            <dt>
                <label for="inputEmail3" class="col-sm-2 control-label">Comentario</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Comp_Comentario)
            </dd>

            <dt>
                <label for="inputEmail3" class="col-sm-2 control-label">Empresa</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.cli_cliente.Cli_Nombre)
            </dd>

            <dt>
                <label for="inputEmail3" class="col-sm-2 control-label">Tecnico Asignado</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.com_usuarios.Com_Nombre)
            </dd>

        </dl>

        <p>
            @Html.ActionLink("Editar", "Edit", new { id = Model.idComp_Bitacora }) |
            @Html.ActionLink("Volver", "Index")
        </p>
    </div>
</div>

